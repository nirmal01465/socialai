{
  "name": "ai-social-super-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite --host 0.0.0.0 --port 5000",
    "dev:server": "nodemon --exec \"node --loader ts-node/esm\" server/app.ts",
    "build": "tsc && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "npm run build:server && node dist/server/app.js",
    "preview": "vite preview --host 0.0.0.0 --port 5000",
    "lint": "tsc --noEmit && tsc --noEmit --project tsconfig.server.json"
  },
  "keywords": [
    "ai",
    "social-media",
    "aggregator",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "description": "AI-powered social media aggregation platform",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.23.12",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "nodemon": "^3.1.10",
    "openai": "^5.16.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  }
}
